{
  "expo": {
    "name": "Rule of Three",
    "slug": "rule-of-three",
    "description": "Calulate rule of 3 math problems effortlessly!",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7746E8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ericquanz.ruleofthree",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#7746E8"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
